<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:right="1dp"
        android:top="1dp"
        android:left="1dp"
        android:bottom="1dp"
        >
        <shape android:shape="rectangle">
            <solid
                android:color="@color/colorAccent"
                />
            <corners android:radius="16dp" />
            <stroke
                android:color="@color/colorAccent"
                android:width="1dp"/>
        </shape>

    </item>
</layer-list>
